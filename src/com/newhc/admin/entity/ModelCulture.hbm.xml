<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="com.newhc.admin.entity.ModelCulture"
        table="model_culture"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="id"
            column="ID"
            type="java.lang.Integer"
            unsaved-value="null"
        >
            <generator class="native">
            </generator>
        </id>
        <property
            name="name"
            type="java.lang.String"
            update="true"
            insert="true"
            column="NAME"
        />
        <property
            name="englishName"
            type="java.lang.String"
            update="true"
            insert="true"
            column="ENGLISH_NAME"
        />
		<property
            name="content"
            type="java.lang.String"
            update="true"
            insert="true"
            column="CONTENT"
        />
         <property
            name="sequence"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="SEQUENCE"
        />
        <property
            name="status"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="STATUS"
        />
       
        <property
            name="createTime"
            type="java.util.Date"
            update="true"
            insert="true"
            column="CREATE_TIME"
        />
		 <property
            name="creator"
            type="java.lang.String"
            update="true"
            insert="true"
            column="CREATOR"
        />
		<property
            name="updateTime"
            type="java.util.Date"
            update="true"
            insert="true"
            column="UPDATE_TIME"
        />
		 <property
            name="updator"
            type="java.lang.String"
            update="true"
            insert="true"
            column="UPDATOR"
        />
        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-Dictionary.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
