<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="com.newhc.admin.entity.ModelColumnImage"
        table="model_column_image"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="id"
            column="ID"
            type="java.lang.Integer"
            unsaved-value="null"
        >
            <generator class="native">
            </generator>
        </id>
		
        
        <property
            name="title"
            type="java.lang.String"
            update="true"
            insert="true"
            column="TITLE"
        />
        
        <property
            name="flash"
            type="java.lang.String"
            update="true"
            insert="true"
            column="FLASH"
        />
        
        <property
            name="link"
            type="java.lang.String"
            update="true"
            insert="true"
            column="LINK"
        />
        
        <property
            name="path"
            type="java.lang.String"
            update="true"
            insert="true"
            column="PATH"
        />
        
        <property
            name="parentType"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="PARENT_TYPE"
        />
        
        <property
            name="type"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="TYPE"
        />
        
        <property
            name="status"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="STATUS"
        />
        
        <property
            name="createTime"
            type="java.util.Date"
            update="true"
            insert="true"
            column="CREATE_TIME"
        />
        
        <property
            name="updateTime"
            type="java.util.Date"
            update="true"
            insert="true"
            column="UPDATE_TIME"
        />
        
        <property
            name="creator"
            type="java.lang.String"
            update="true"
            insert="true"
            column="CREATOR"
        />
        
        <property
            name="updator"
            type="java.lang.String"
            update="true"
            insert="true"
            column="UPDATOR"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-Dictionary.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
