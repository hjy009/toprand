<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="com.newhc.admin.entity.ModelVideo"
        table="model_video"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="id"
            column="ID"
            type="java.lang.Integer"
            unsaved-value="null"
        >
            <generator class="native">
            </generator>
        </id>
        <property
            name="categoryId"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="CATEGORY_ID"
        />
        <property
            name="name"
            type="java.lang.String"
            update="true"
            insert="true"
            column="NAME"
        />

        <property
            name="path"
            type="java.lang.String"
            update="true"
            insert="true"
            column="PATH"
        />
        
        <property
            name="imagePath"
            type="java.lang.String"
            update="true"
            insert="true"
            column="IMAGE_PATH"
        />
        
        <property
            name="remark"
            type="java.lang.String"
            update="true"
            insert="true"
            column="REMARK"
        />
   

			<property
	            name="pubTime"
	            type="java.util.Date"
	            update="true"
	            insert="true"
	            column="PUB_TIME"
	        />
	        
        <property
            name="status"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="STATUS"
        />
        
        <property
            name="creator"
            type="java.lang.String"
            update="true"
            insert="true"
            column="CREATOR"
        />
        
        <property
            name="updator"
            type="java.lang.String"
            update="true"
            insert="true"
            column="UPDATOR"
        />
        <property
            name="createTime"
            type="java.util.Date"
            update="true"
            insert="true"
            column="CREATE_TIME"
        />
        
        <property
            name="updateTime"
            type="java.util.Date"
            update="true"
            insert="true"
            column="UPDATE_TIME"
        />
        


        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-Dictionary.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
